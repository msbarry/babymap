{
  "name": "react-map",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "gen": "./scripts/gen-map.sh",
    "lint": "eslint --ext js,jsx . && echo 'no lint errors!'",
    "lint:watch": "npm run lint -s; onchange src scripts *.js *.jsx .eslint* -- npm run lint -s",
    "start": "parallelshell 'npm run lint:watch' 'npm run serve:dev'",
    "preserve:prod": "npm run build",
    "serve:prod": "NODE_ENV=production http-server dist/ -o",
    "serve:dev": "NODE_ENV=development webpack-dev-server --inline --open --no-info --host 0.0.0.0 --port 3000",
    "prebuild": "npm run clean -s && npm run lint -s && npm test -s",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress --colors -p",
    "deploy": "npm run build && ./scripts/deploy.sh"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-root-import": "^4.1.5",
    "css-loader": "^0.26.0",
    "d3-array": "^1.0.2",
    "d3-axis": "^1.0.4",
    "d3-color": "^1.0.2",
    "d3-drag": "^1.0.2",
    "d3-dsv": "^1.0.3",
    "d3-ease": "^1.0.2",
    "d3-geo": "^1.4.0",
    "d3-interpolate": "^1.1.2",
    "d3-scale": "^1.0.4",
    "d3-selection": "^1.0.3",
    "d3-shape": "^1.0.4",
    "d3-timer": "^1.0.3",
    "d3-transition": "^1.0.3",
    "distinct-colors": "^1.0.4",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "iwanthue-api": "^0.1.0",
    "json-loader": "^0.5.4",
    "normalize.css": "^5.0.0",
    "onchange": "^3.0.2",
    "opener": "^1.4.2",
    "parallelshell": "^2.0.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^2.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "text-loader": "^0.0.1",
    "topojson": "1",
    "topojson-client": "^2.1.0",
    "topojson-simplify": "^2.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "welshpowell": "^1.0.1"
  }
}
